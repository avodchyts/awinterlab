<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.Db.LandtransportMapper">

    <select id="getLandtransport" resultType="Landtransport">
        select * from landtransport where idLandtransport = #{idLandtransport}

    </select>


    <select id="getLandtransports" resultType="Landtransport">
        select * from landtransport

    </select>

</mapper>