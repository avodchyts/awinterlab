package com.InsVihcle;

public class Vihcle {
    private String typeVihcle;
    private int valueVihcle;
    private int yearProduce;

    public Vihcle () {}
    public Vihcle ( String typeVihcle, int valueVihcle,int yearProduce ){
        this.typeVihcle = typeVihcle;
        this.valueVihcle = valueVihcle;
        this.yearProduce = yearProduce;
    }
    public void setVihcle (String typeVihcle, int valueVihcle, int yearProduce){
        this.typeVihcle = typeVihcle;
        this.valueVihcle = valueVihcle;
        this.yearProduce = yearProduce;
            }
            public void getVihcle (){
        System.out.println("typeVihcle - " + this.typeVihcle );
        System.out.println("valueVihcle -" + valueVihcle );
        System.out.println("yearProduce - " + yearProduce);
            }
}